{
  "name": "laspi94/idl-mailing",
  "version": "1.0.1",
  "description": "Package para realizar llamada al api de IDL S.A para envio de mails personalizados.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "files": [
      "src/classes/MailingBuilder.php"
    ],
    "psr-4": {
      "Idl\\Mailing\\": "src/",
      "Idl\\Mailing\\Class\\": "src/classes/",
      "Idl\\Mailing\\Trait\\": "src/traits/",
      "Idl\\Mailing\\Helpers\\": "src/helpers/"
    }
  },
  "authors": [
    {
      "name": "laspi94",
      "email": "alanjoselaspina@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Idl\\Mailing\\MailingServiceProvider"
      ]
    }
  }
}
